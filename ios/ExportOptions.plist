<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Distribution method: app-store (TestFlight/App Store), ad-hoc, enterprise, or development -->
    <key>method</key>
    <string>app-store</string>

    <!-- Your Apple Developer Team ID (10-character alphanumeric) -->
    <key>teamID</key>
    <string>YOUR_TEAM_ID</string>

    <!-- Bundle ID must match your app's identifier -->
    <key>bundleIdentifier</key>
    <string>com.stickerguard</string>

    <!-- Bitcode is deprecated for iOS apps -->
    <key>uploadBitcode</key>
    <false/>
    <key>compileBitcode</key>
    <false/>

    <!-- Upload symbols for crash reporting (recommended) -->
    <key>uploadSymbols</key>
    <true/>

    <!-- Use manual signing for CI/CD environments -->
    <key>signingStyle</key>
    <string>manual</string>

    <!-- Provisioning profile mapping -->
    <key>provisioningProfiles</key>
    <dict>
        <key>com.stickerguard</key>
        <string>YOUR_PROVISIONING_PROFILE_NAME</string>
    </dict>

    <!-- Code signing identity -->
    <key>signingCertificate</key>
    <string>iPhone Distribution</string>

    <!-- Manage version and build number (optional) -->
    <key>manageAppVersionAndBuildNumber</key>
    <false/>
</dict>
</plist>
